    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@jest/globals": "^27.2.5",
    "@material-ui/pickers": "^3.3.10",
    "@material-ui/core": "^4.12.3",
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/react": "^6.3.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@vitejs/plugin-react": "^1.0.2",
    "framer-motion": "^4",
    "react": "^17.0.2",
    "react-currency-input-field": "^3.6.0",
    "react-datepicker": "^4.3.0",
    "react-dom": "^17.0.2",
    "react-infinite-scroll-component": "^6.1.0",
    "react-plaid-link": "^3.2.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "vite": "^2.6.3",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@storybook/node-logger": "^6.3.6",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/history": "^4.7.7",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.14.37",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-imgix": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-mdx": "^1.7.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^2.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "mdx-loader": "^3.0.2",
    "mocha": "5.2.0",
    "mochawesome": "^4.1.0",
    "mochawesome-merge": "^2.0.1",
    "mochawesome-report-generator": "^4.0.1",
    "postcss": "^8.2.1",
    "postcss-loader": "^4.2.0",
    "prettier": "^1.19.1",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.2"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "deploy": "vite build",
    "serve": "vite preview",
    "storybook": "start-storybook -p 6006 -s public",
    "pre-push": "rm -rf storybook-static/ && build-storybook -s public && vite build",
    "build-storybook": "build-storybook -s public",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "eslint src/*.{js,jsx,ts,tsx} --fix-dry-run --no-error-on-unmatched-pattern",
      "pnpm run test"
    ]
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "./jest.config.js"
      }
    }
  }
}
